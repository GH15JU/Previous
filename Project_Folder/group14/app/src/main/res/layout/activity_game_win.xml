<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="calculotprototype.g14.cmpt276.calculot_prototype.calcGame.GameWinActivity">


    <RelativeLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
    >

        <TextView
            android:id="@+id/gamewin_xpfield"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            tools:layout_conversion_absoluteHeight="132dp"
            tools:layout_conversion_absoluteWidth="5dp"
            tools:layout_conversion_absoluteX="378dp"
            tools:layout_conversion_absoluteY="29dp"
            tools:layout_editor_absoluteX="378dp"
            tools:layout_editor_absoluteY="29dp"
            android:layout_marginTop="30dp"
            android:layout_below="@+id/gamewin_WinMessage"
            android:layout_alignParentStart="true" />

        <TextView
            android:id="@+id/gamewin_WinMessage"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/WinMessage"
            tools:layout_conversion_absoluteHeight="511dp"
            tools:layout_conversion_absoluteWidth="5dp"
            tools:layout_conversion_absoluteX="373dp"
            tools:layout_conversion_absoluteY="29dp"
            tools:layout_editor_absoluteX="373dp"
            tools:layout_editor_absoluteY="29dp"
            android:layout_below="@+id/gamewin_Header"
            android:layout_alignParentStart="true"
            android:layout_marginTop="13dp" />

        <TextView
            android:id="@+id/gamewin_Header"
            android:layout_width="373dp"
            android:layout_height="wrap_content"
            android:text="@string/Congratulations"
            android:textAppearance="@style/TextAppearance.AppCompat.Display2"
            tools:layout_conversion_absoluteHeight="53dp"
            tools:layout_conversion_absoluteWidth="373dp"
            tools:layout_conversion_absoluteX="0dp"
            tools:layout_conversion_absoluteY="0dp"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp" />
    </RelativeLayout>


</android.support.constraint.ConstraintLayout>
